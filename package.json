{
  "name": "ai-novel-game-generator",
  "version": "1.0.0",
  "description": "AI powered novel game generator web service",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client & npm start",
    "build": "cd client && CI=false npm run build",
    "install-all": "npm install & cd client & npm install",
    "railway-build": "npm install && cd client && npm install && npm run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "multer": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "ai",
    "novel",
    "game",
    "generator",
    "web",
    "react",
    "node"
  ],
  "author": "",
  "license": "MIT"
}
